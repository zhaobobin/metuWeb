{
  "name": "metuWeb",
  "private": true,
  "scripts": {
    "start": "cross-env X_ENV=dev umi dev",
    "start:dev": "cross-env X_ENV=dev umi dev",
    "start:pro": "cross-env X_ENV=pro umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "@ant-design/pro-layout": "^6.5.0",
    "antd": "^5.1.4",
    "antd-mobile": "^5.27.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "metu-ui": "^0.1.1",
    "qrcode.react": "^3.1.0",
    "react": "17.x",
    "react-cropper": "^2.1.8",
    "react-dom": "17.x",
    "react-grid-layout": "^1.2.5",
    "react-infinite-scroller": "^1.2.4",
    "react-lazyload": "^3.2.0",
    "react-masonry-component": "^6.3.0",
    "react-measure": "^2.5.2",
    "react-photo-gallery": "6.1.4",
    "screenfull": "^5.1.0",
    "share-react": "^1.0.0",
    "umi": "^3.5.13",
    "wxlogin.react": "^0.1.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-infinite-scroller": "^1.2.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.13",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "open-browser-webpack-plugin": "^0.0.5",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
